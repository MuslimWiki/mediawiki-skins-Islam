<li class="islam-typeahead-list-item">
	<a
		href="{{href}}"
		class="islam-typeahead-list-item-link"
		id="islam-typeahead-item-{{type}}-{{id}}"
	>
		{{#icon}}<div class="islam-typeahead-list-item-icon islam-ui-icon mw-ui-icon-wikimedia-{{.}}"></div>{{/icon}}
		{{#image}}
		<div
			class="islam-typeahead-list-item-image{{#image.class}} {{.}}{{/image.class}}"
			{{#image.url}}style="background-image: url({{.}});"{{/image.url}}
		>
		</div>
		{{/image}}
		<div class="islam-typeahead-list-item-text">
			{{#text}}{{.}}{{/text}}
			{{#title}}<div class="islam-typeahead-list-item-title">{{{.}}}</div>{{/title}}
			{{#description}}<div class="islam-typeahead-list-item-description">{{.}}</div>{{/description}}
		</div>
		{{#html-end}}<div class="islam-typeahead-list-item-end">{{{.}}}</div>{{/html-end}}
	</a>
</li>